  font-size: 2.25rem;
  line-height: 2.625rem;
}

h2 {
  font-size: 1.56rem;
  line-height: 1.75rem;
}

h3 {
    font-size: 1rem;
    line-height: 1.26rem;
}

p {
  line-height: 1.26rem;
}

a {
    color: white;
    text-decoration: underline;
